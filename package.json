{
  "name": "video-blogs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "zzdev": "NEXT_DISABLE_TYPE_CHECKING=true next dev",
    "migrate:all": "npx prisma migrate dev --name $npm_config_name && npx prisma generate",
    "db:reset": "npx prisma migrate reset",
    "dev": "next dev --turbo",
    "debug": "NODE_OPTIONS='--inspect' next dev --turbo",
    "dev:server": "NODE_ENV=development next dev --turbo --port 3000",
    "dev-old": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/server.ts",
    "plop": "cross-env NODE_OPTIONS=\"--import tsx\" plop --plopfile=plopfile.ts",
    "debug:plop": "cross-env NODE_OPTIONS=\"--import tsx --inspect-brk\" plop --plopfile=plopfile.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "cy:open": "cypress open",
    "typecheck": "tsc",
    "typecheck:watch": "tsc -w"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.mod.commonjs.json prisma/seed/index.ts"
  },
  "dependencies": {
    "@babel/standalone": "^7.26.10",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.10",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "googleapis": "^152.0.0",
    "lucide-react": "^0.525.0",
    "next": "^15.2.3",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.17.24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.2",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
